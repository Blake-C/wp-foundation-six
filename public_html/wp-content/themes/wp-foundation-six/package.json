{
  "name": "wp-foundation-six",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "12.18.0"
  },
  "browserslist": [
    "> 1% or last 3 versions"
  ],
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "acorn": "^7.2.0",
    "ajv": "^6.12.2",
    "autoprefixer": "^9.8.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "2.26.7",
    "cssnano": "^4.1.10",
    "del": "5.1.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "gulp": "4.0.2",
    "gulp-babel": "8.0.0",
    "gulp-concat": "2.6.1",
    "gulp-imagemin": "7.1.0",
    "gulp-load-plugins": "^2.0.3",
    "gulp-newer": "1.4.0",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "8.0.0",
    "gulp-rename": "2.0.0",
    "gulp-rsync": "0.0.9",
    "gulp-sass": "^4.1.0",
    "gulp-shell": "^0.8.0",
    "gulp-size": "3.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "3.0.2",
    "imagemin-gifsicle": "7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.0",
    "imagemin-svgo": "^8.0.0",
    "prettier": "^2.0.5",
    "snyk": "^1.336.0",
    "stylelint": "^13.6.0",
    "stylelint-config-wordpress": "^17.0.0",
    "stylelint-order": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "what-input": "^5.2.10",
    "yargs": "^15.3.1"
  },
  "dependencies": {
    "detect-browser": "5.1.1",
    "foundation-sites": "^6.6.3",
    "jquery": "^3.5.1",
    "jquery-migrate": "^3.3.0",
    "modernizr": "^3.11.2"
  },
  "stylelint": {
    "plugins": [
      "stylelint-order"
    ],
    "extends": [
      "stylelint-config-wordpress/scss"
    ],
    "rules": {
      "order/properties-alphabetical-order": true,
      "string-quotes": "single",
      "color-no-invalid-hex": true,
      "declaration-no-important": true,
      "selector-no-vendor-prefix": true,
      "no-descending-specificity": null,
      "selector-max-id": 0,
      "function-url-quotes": "always",
      "at-rule-empty-line-before": [
        "always",
        {
          "except": [
            "blockless-after-blockless"
          ],
          "ignore": [
            "after-comment"
          ],
          "ignoreAtRules": [
            "include",
            "extend",
            "import",
            "if",
            "else",
            "mixin",
            "at-root"
          ]
        }
      ],
      "rule-empty-line-before": [
        "always",
        {
          "ignore": [
            "after-comment",
            "first-nested"
          ]
        }
      ]
    }
  }
}
